package com.inetum.appliSpring.jpa.init;

import javax.annotation.PostConstruct;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.inetum.appliSpringWeb.dao.DaoCompte;
import com.inetum.appliSpringWeb.entity.Compte;



@Component
public class InitDataSet {

	@Autowired
	private DaoCompte daoCompteJpa;
	
	@PostConstruct
	public void initData() {
		daoCompteJpa.insert(new Compte(null, "A1",50.0));
		daoCompteJpa.insert(new Compte(null, "A2",80.0));
		
	}
}
